{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "Me!"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "MIT",
    "icon": "assets/flatbedrock/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.chimericdream.flatbedrock.fabric.FlatBedrockFabric"
        ],
        "client": [
            "com.chimericdream.flatbedrock.fabric.client.FlatBedrockFabricClient"
        ]
    },
    "mixins": [
        "flatbedrock.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_compat}",
        "minecraft": "~${minecraft_compat}",
        "java": ">=21",
        "fabric-api": "*",
        "architectury": ">=${architectury_compat}"
    },
    "recommends": {
        "modmenu": ">=${modmenu_compat}"
    }
}
